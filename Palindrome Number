class Solution {
    public boolean isPalindrome(int x) {
        int n = 0;
        int k = x;
        int i =0;
        int  p = x;
        int a = 0;
        while(p>0){
            a++;
            p = p/10;
        }
        while(k>0)
        {
            int m = k % 10;
            n = n + (int)(m*Math.pow(10,a-1));
            a--;
            k = k/10;
        }
        if(x==n) return true ;
        else return false;
    }
}
