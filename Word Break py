class Solution:
    def wordBreak(self, s: str, wordDict: List[str]) -> bool:
        
        def dfs(s, memo):
            if s == "":
                return True 
            if s in memo:
                return memo[s]
            for i in wordDict:
                # print(st)
                if s.startswith(i):
                    if dfs(s[len(i):], memo):
                        memo[i] = True
                        return True
            memo[s] = False    
            return False    
                
        return dfs(s, {})         
